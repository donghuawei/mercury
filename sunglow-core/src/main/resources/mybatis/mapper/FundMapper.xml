<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aqitrade.sunglow.core.mapper.FundMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.aqitrade.sunglow.core.model.Fund">
        <id column="fund_id" property="fundId" />
        <result column="create_date" property="createDate" />
        <result column="name" property="name" />
        <result column="amount" property="amount" />
        <result column="description" property="description" />
        <result column="policy_id" property="policyId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        fund_id, create_date, name, amount, description, policy_id
    </sql>

</mapper>
