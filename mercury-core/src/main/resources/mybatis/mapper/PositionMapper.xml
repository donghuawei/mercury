<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aqitrade.mercury.core.mapper.PositionMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.aqitrade.mercury.core.model.Position">
        <id column="position_id" property="positionId" />
        <result column="create_date" property="createDate" />
        <result column="avg_price" property="avgPrice" />
        <result column="qty" property="qty" />
        <result column="instrument_id" property="instrumentId" />
        <result column="fund_id" property="fundId" />
        <result column="instrument_type" property="instrumentType" />
        <result column="account_id" property="accountId" />
        <result column="transaction_id" property="transactionId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        position_id, create_date, avg_price, qty, instrument_id, fund_id, instrument_type, account_id, transaction_id
    </sql>

</mapper>
